import turtle           # importo biblioteca llamada turtle desde Python

t = turtle.Turtle ()    # creo t (abreviaci처n de turtle)

t.shape ("turtle")      # uso cursor con figura de tortuga

t.forward (100)         # avanzo 100
t.left (90)             # giro hacia la izquierda en 90째
t.forward (100)         # avanzo 100
t.left (90)             # giro hacia la izquierda en 90째
t.forward (100)         # avanzo 100
t.left (90)             # giro hacia la izquierda en 90째
t.forward (100)         # avanzo 100
